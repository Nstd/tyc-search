{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "packager": "electron-packager . app --win --out tycTool --arch=x64 --app-version=1.0 --overwrite",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --win --mac --ia32"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^5.0.2",
    "electron-builder": "^20.41.0"
  },
  "build": {
    "appId": "com.hzt.tyc",
    "productName": "tycTool",
    "asar": true,
    "compression": "normal",
    "directories": {
      "buildResources": "build"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "dependencies": {
    "axios": "^0.19.0",
    "cheerio": "^1.0.0-rc.3",
    "request": "^2.88.0"
  }
}
